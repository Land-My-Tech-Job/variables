// # Problem Two
let currentAge = 34
let maxAge = 40
let snackAmount = 4 

 
let lifetimeSnacks = (maxAge - currentAge) * snackAmount
console.log('You will need ' + lifetimeSnacks + ' to last you until the age of ' + maxAge)














